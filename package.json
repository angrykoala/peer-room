{
  "name": "socket-stream",
  "version": "0.1.0",
  "description": "WebRTC + Sockets for a lovely P2P",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "rm -rf dist && tsc",
    "tslint": "tslint *.ts src/*.ts src/*/*.ts src/*/*/*.ts examples/*.ts --fix",
    "examples-frontend": "parcel watch examples/*.html -d examples/dist",
    "examples-server": "ts-node examples/server.ts",
    "examples": "concurrently --kill-others \"npm run examples-frontend\" \"npm run examples-server\"",
    "prepublishOnly": "npm run tslint && npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angrykoala/socket-stream.git"
  },
  "files": [
    "dist"
  ],
  "types": "dist/main.d.ts",
  "keywords": [
    "webrtc",
    "socket",
    "p2p",
    "stream"
  ],
  "browserslist": [
    "last 1 Chrome version"
  ],
  "author": "angrykoala <angrykoala@outlook.es>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/angrykoala/socket-stream/issues"
  },
  "homepage": "https://github.com/angrykoala/socket-stream#readme",
  "dependencies": {
    "simple-peer": "^9.7.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.22",
    "@types/simple-peer": "^9.6.0",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "parcel": "^1.12.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  }
}
